            "k= 10\n",
            "a= 25\n",
            "K+a= 35\n",
            "k= 35\n",
            "a= 26\n",
            "K+a= 61\n",
            "k= 61\n",
            "a= 36\n",
            "K+a= 97\n",
            "k= 97\n",
